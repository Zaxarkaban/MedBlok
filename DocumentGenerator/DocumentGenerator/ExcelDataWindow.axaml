<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:DocumentGenerator.ViewModels"
        x:Class="DocumentGenerator.ExcelDataWindow"
        x:DataType="vm:ExcelDataViewModel"
        Title="Excel Data" Width="800" Height="600">
	<StackPanel>
		<TextBlock Text="Данные из Excel" FontSize="16" Margin="10"/>
		<DataGrid ItemsSource="{Binding PeopleData}" AutoGenerateColumns="False" Margin="10">
			<DataGrid.Columns>
				<DataGridTextColumn Header="ФИО" Binding="{Binding [FullName]}"/>
				<DataGridTextColumn Header="Должность" Binding="{Binding [Position]}"/>
				<DataGridTextColumn Header="Дата рождения" Binding="{Binding [DateOfBirth]}"/>
				<DataGridTextColumn Header="Пол" Binding="{Binding [Gender]}"/>
				<DataGridTextColumn Header="СНИЛС" Binding="{Binding [Snils]}"/>
				<DataGridTextColumn Header="Серия паспорта" Binding="{Binding [PassportSeries]}"/>
				<DataGridTextColumn Header="Номер паспорта" Binding="{Binding [PassportNumber]}"/>
				<DataGridTextColumn Header="Дата выдачи паспорта" Binding="{Binding [PassportIssueDate]}"/>
				<DataGridTextColumn Header="Кем выдан паспорт" Binding="{Binding [PassportIssuedBy]}"/>
				<DataGridTextColumn Header="Полис ОМС" Binding="{Binding [MedicalPolicy]}"/>
				<DataGridTextColumn Header="Адрес" Binding="{Binding [Address]}"/>
				<DataGridTextColumn Header="Телефон" Binding="{Binding [Phone]}"/>
				<DataGridTextColumn Header="Мед. организация" Binding="{Binding [MedicalOrganization]}"/>
				<DataGridTextColumn Header="ЛПУ" Binding="{Binding [MedicalFacility]}"/>
				<DataGridTextColumn Header="Место работы" Binding="{Binding [Workplace]}"/>
				<DataGridTextColumn Header="Форма собственности" Binding="{Binding [OwnershipForm]}"/>
				<DataGridTextColumn Header="ОКВЭД" Binding="{Binding [Okved]}"/>
				<DataGridTextColumn Header="Стаж работы" Binding="{Binding [WorkExperience]}"/>
				<DataGridTextColumn Header="Пункты вредности" Binding="{Binding [OrderClause]}"/>
				<DataGridTextColumn Header="Список врачей" Binding="{Binding [DoctorsList]}"/>
			</DataGrid.Columns>
		</DataGrid>
	</StackPanel>
</Window>