<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:DocumentGenerator.ViewModels"
        x:Class="DocumentGenerator.MainWindow"
        x:DataType="vm:MainWindowViewModel"
        Title="Профосмотры" Width="450" Height="700">
	<!-- Многослойный фон (без анимации, затемнённые цвета) -->
	<Window.Background>
		<LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
			<GradientStop Color="#04261f" Offset="0"/>
			<!-- Затемнённый #063d2c -->
			<GradientStop Color="#0d3b31" Offset="0.5"/>
			<!-- Затемнённый #125e47 -->
			<GradientStop Color="#04261f" Offset="1"/>
			<!-- Затемнённый #063d2c -->
		</LinearGradientBrush>
	</Window.Background>
	<!-- Стили -->
	<Window.Styles>
		<!-- Стиль для TextBlock (метки) -->
		<Style Selector="TextBlock">
			<Setter Property="Foreground" Value="#d9e1e8"/>
			<!-- Затемнённый #f0f4f8 -->
			<Setter Property="FontSize" Value="14"/>
			<!-- Уменьшено с 16 -->
			<Setter Property="FontWeight" Value="Medium"/>
			<Setter Property="FontFamily" Value="Segoe UI, Roboto, Arial"/>
			<Setter Property="Margin" Value="0,5,0,3"/>
			<!-- Уменьшено с 0,10,0,5 -->
			<Setter Property="Effect">
				<Setter.Value>
					<DropShadowEffect Color="Black" BlurRadius="3" OffsetX="1" OffsetY="1" Opacity="0.5"/>
				</Setter.Value>
			</Setter>
		</Style>
		<!-- Стиль для TextBlock с ошибками -->
		<Style Selector="TextBlock[Foreground=Red]">
			<Setter Property="FontSize" Value="14"/>
			<!-- Уменьшено с 14 -->
			<Setter Property="Margin" Value="0,2,0,0"/>
			<!-- Уменьшено с 0,2,0,10 -->
			<Setter Property="Effect">
				<Setter.Value>
					<DropShadowEffect Color="Black" BlurRadius="2" OffsetX="1" OffsetY="1" Opacity="0.3"/>
				</Setter.Value>
			</Setter>
		</Style>
		<!-- Стиль для TextBox -->
		<Style Selector="TextBox">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#1f8f7a" Offset="0"/>
						<!-- Затемнённый #2ed1b5 -->
						<GradientStop Color="#073f36" Offset="1"/>
						<!-- Затемнённый #0a5c4e -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Foreground" Value="White"/>
			<Setter Property="FontSize" Value="14"/>
			<!-- Уменьшено с 16 -->
			<Setter Property="FontFamily" Value="Segoe UI, Roboto, Arial"/>
			<Setter Property="CornerRadius" Value="8"/>
			<!-- Уменьшено с 10 -->
			<Setter Property="Padding" Value="8"/>
			<!-- Уменьшено с 10 -->
			<Setter Property="Margin" Value="0,3,0,3"/>
			<!-- Уменьшено с 0,5,0,5 -->
			<Setter Property="Opacity" Value="0.9"/>
			<Setter Property="Effect">
				<Setter.Value>
					<DropShadowEffect Color="Black" BlurRadius="4" OffsetX="2" OffsetY="2" Opacity="0.4"/>
					<!-- Уменьшено с 5 -->
				</Setter.Value>
			</Setter>
			<Setter Property="Transitions">
				<Transitions>
					<BrushTransition Property="Background" Duration="0:0:0.3"/>
					<DoubleTransition Property="Opacity" Duration="0:0:0.3"/>
				</Transitions>
			</Setter>
		</Style>
		<!-- Стиль для TextBox при наведении -->
		<Style Selector="TextBox:pointerover">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#2ab091" Offset="0"/>
						<!-- Затемнённый #3ef7d2 -->
						<GradientStop Color="#116d5a" Offset="1"/>
						<!-- Затемнённый #1a9b82 -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Opacity" Value="1"/>
		</Style>
		<!-- Стиль для ComboBox -->
		<Style Selector="ComboBox">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#1f8f7a" Offset="0"/>
						<!-- Затемнённый #2ed1b5 -->
						<GradientStop Color="#073f36" Offset="1"/>
						<!-- Затемнённый #0a5c4e -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Foreground" Value="White"/>
			<Setter Property="FontSize" Value="14"/>
			<!-- Уменьшено с 16 -->
			<Setter Property="FontFamily" Value="Segoe UI, Roboto, Arial"/>
			<Setter Property="CornerRadius" Value="8"/>
			<!-- Уменьшено с 10 -->
			<Setter Property="Padding" Value="8"/>
			<!-- Уменьшено с 10 -->
			<Setter Property="Margin" Value="0,3,0,3"/>
			<!-- Уменьшено с 0,5,0,5 -->
			<Setter Property="Opacity" Value="0.9"/>
			<Setter Property="Effect">
				<Setter.Value>
					<DropShadowEffect Color="Black" BlurRadius="4" OffsetX="2" OffsetY="2" Opacity="0.4"/>
					<!-- Уменьшено с 5 -->
				</Setter.Value>
			</Setter>
			<Setter Property="Transitions">
				<Transitions>
					<BrushTransition Property="Background" Duration="0:0:0.3"/>
					<DoubleTransition Property="Opacity" Duration="0:0:0.3"/>
				</Transitions>
			</Setter>
		</Style>
		<!-- Стиль для ComboBox при наведении -->
		<Style Selector="ComboBox:pointerover">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#2ab091" Offset="0"/>
						<!-- Затемнённый #3ef7d2 -->
						<GradientStop Color="#116d5a" Offset="1"/>
						<!-- Затемнённый #1a9b82 -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Opacity" Value="1"/>
		</Style>
		<!-- Стиль для ListBox -->
		<Style Selector="ListBox">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#1f8f7a" Offset="0"/>
						<!-- Затемнённый #2ed1b5 -->
						<GradientStop Color="#073f36" Offset="1"/>
						<!-- Затемнённый #0a5c4e -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Foreground" Value="White"/>
			<Setter Property="FontSize" Value="14"/>
			<!-- Уменьшено с 16 -->
			<Setter Property="FontFamily" Value="Segoe UI, Roboto, Arial"/>
			<Setter Property="CornerRadius" Value="8"/>
			<!-- Уменьшено с 10 -->
			<Setter Property="Padding" Value="8"/>
			<!-- Уменьшено с 10 -->
			<Setter Property="Margin" Value="0,3,0,3"/>
			<!-- Уменьшено с 0,5,0,5 -->
			<Setter Property="Opacity" Value="0.9"/>
			<Setter Property="Effect">
				<Setter.Value>
					<DropShadowEffect Color="Black" BlurRadius="4" OffsetX="2" OffsetY="2" Opacity="0.4"/>
					<!-- Уменьшено с 5 -->
				</Setter.Value>
			</Setter>
			<Setter Property="Transitions">
				<Transitions>
					<BrushTransition Property="Background" Duration="0:0:0.3"/>
					<DoubleTransition Property="Opacity" Duration="0:0:0.3"/>
				</Transitions>
			</Setter>
		</Style>
		<!-- Стиль для ListBox при наведении -->
		<Style Selector="ListBox:pointerover">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#2ab091" Offset="0"/>
						<!-- Затемнённый #3ef7d2 -->
						<GradientStop Color="#116d5a" Offset="1"/>
						<!-- Затемнённый #1a9b82 -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Opacity" Value="1"/>
		</Style>
		<!-- Стиль для кнопок -->
		<Style Selector="Button">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#1f8f7a" Offset="0"/>
						<!-- Затемнённый #2ed1b5 -->
						<GradientStop Color="#073f36" Offset="1"/>
						<!-- Затемнённый #0a5c4e -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Foreground" Value="White"/>
			<Setter Property="FontSize" Value="16"/>
			<!-- Уменьшено с 18 -->
			<Setter Property="FontWeight" Value="Bold"/>
			<Setter Property="FontFamily" Value="Segoe UI, Roboto, Arial"/>
			<Setter Property="Width" Value="200"/>
			<!-- Уменьшено с 300 -->
			<Setter Property="Height" Value="50"/>
			<!-- Уменьшено с 60 -->
			<Setter Property="Margin" Value="0,10"/>
			<!-- Уменьшено с 0,15 -->
			<Setter Property="CornerRadius" Value="25"/>
			<!-- Уменьшено с 30 -->
			<Setter Property="HorizontalAlignment" Value="Center"/>
			<Setter Property="HorizontalContentAlignment" Value="Center"/>
			<Setter Property="VerticalContentAlignment" Value="Center"/>
			<Setter Property="Opacity" Value="0.9"/>
			<Setter Property="Effect">
				<Setter.Value>
					<DropShadowEffect Color="Black" BlurRadius="8" OffsetX="3" OffsetY="3" Opacity="0.5"/>
					<!-- Уменьшено с 10 -->
				</Setter.Value>
			</Setter>
			<Setter Property="Transitions">
				<Transitions>
					<BrushTransition Property="Background" Duration="0:0:0.3"/>
					<DoubleTransition Property="Opacity" Duration="0:0:0.3"/>
				</Transitions>
			</Setter>
		</Style>
		<!-- Стиль при наведении -->
		<Style Selector="Button:pointerover">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#2ab091" Offset="0"/>
						<!-- Затемнённый #3ef7d2 -->
						<GradientStop Color="#116d5a" Offset="1"/>
						<!-- Затемнённый #1a9b82 -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Opacity" Value="1"/>
		</Style>
		<!-- Стиль при нажатии -->
		<Style Selector="Button:pressed">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Color="#0a584a" Offset="0"/>
						<!-- Затемнённый #0f7a6b -->
						<GradientStop Color="#04261f" Offset="1"/>
						<!-- Затемнённый #063d2c -->
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Opacity" Value="0.8"/>
		</Style>
	</Window.Styles>
	<!-- Основной контент -->
	<Design.DataContext>
		<vm:MainWindowViewModel />
	</Design.DataContext>
	<ScrollViewer>
		<StackPanel Margin="10,10,20,10" Name="MainStackPanel">
			<TextBlock Text="Поликлиническое отделение" />
			<ComboBox Name="ServicePointComboBox"
                      SelectedItem="{Binding ServicePoint, Mode=TwoWay}"
                      ItemsSource="{Binding ServicePointOptions}"
                      SelectionChanged="ComboBox_SelectionChanged"
                      GotFocus="ComboBox_GotFocus"
                      KeyDown="InputField_KeyDown"
                      TabIndex="0" />
			<TextBlock Text="{Binding ServicePointError}" Foreground="Red" />

			<TextBlock Text="ФИО" />
			<TextBox Name="FullNameTextBox"
                     Text="{Binding FullName, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="Введите ФИО"
                     TabIndex="1" />
			<TextBlock Text="{Binding FullNameError}" Foreground="Red" />

			<TextBlock Text="Должность" />
			<TextBox Name="PositionTextBox"
                     Text="{Binding Position, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="Введите должность"
                     TabIndex="2" />
			<TextBlock Text="{Binding PositionError}" Foreground="Red" />

			<TextBlock Text="Дата рождения (ДД.ММ.ГГГГ)" />
			<TextBox Name="DateOfBirthTextBox"
                     Text="{Binding DateOfBirth, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="ДД.ММ.ГГГГ"
                     TabIndex="3" />
			<TextBlock Text="{Binding DateOfBirthError}" Foreground="Red" />

			<TextBlock Text="Пол" />
			<ComboBox Name="GenderComboBox"
                      SelectedItem="{Binding Gender}"
                      ItemsSource="{Binding GenderOptions}"
                      SelectionChanged="ComboBox_SelectionChanged"
                      GotFocus="ComboBox_GotFocus"
                      KeyDown="InputField_KeyDown"
                      TabIndex="4" />
			<TextBlock Text="{Binding GenderError}" Foreground="Red" />

			<TextBlock Text="Пункты вредности (Выбрать несколько crtl+ЛКМ)" />
			<TextBox Name="OrderClausesSearchBox"
                     Text="{Binding OrderClausesSearchText, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleSearchTextChanged"
                     Watermark="Поиск пунктов..."
                     KeyDown="InputField_KeyDown"
                     TabIndex="5" />
			<ListBox Name="OrderClausesListBox"
                     ItemsSource="{Binding FilteredOrderClauseOptions}"
                     SelectedItems="{Binding SelectedOrderClauses}"
                     SelectionMode="Multiple"
                     Height="200"
                     TabIndex="6" />
			<TextBlock Text="{Binding SelectedOrderClausesError}" Foreground="Red" />

			<TextBlock Text="СНИЛС (XXX-XXX-XXX XX)" />
			<TextBox Name="SnilsTextBox"
                     Text="{Binding Snils, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="XXX-XXX-XXX XX"
                     TabIndex="7" />
			<TextBlock Text="{Binding SnilsError}" Foreground="Red" />

			<TextBlock Text="Серия паспорта (XXXX)" />
			<TextBox Name="PassportSeriesTextBox"
                     Text="{Binding PassportSeries, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="XXXX"
                     TabIndex="8" />
			<TextBlock Text="{Binding PassportSeriesError}" Foreground="Red" />

			<TextBlock Text="Номер паспорта (XXXXXX)" />
			<TextBox Name="PassportNumberTextBox"
                     Text="{Binding PassportNumber, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="XXXXXX"
                     TabIndex="9" />
			<TextBlock Text="{Binding PassportNumberError}" Foreground="Red" />

			<TextBlock Text="Дата выдачи паспорта (ДД.ММ.ГГГГ)" />
			<TextBox Name="PassportIssueDateTextBox"
                     Text="{Binding PassportIssueDate, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="ДД.ММ.ГГГГ"
                     TabIndex="10" />
			<TextBlock Text="{Binding PassportIssueDateError}" Foreground="Red" />

			<TextBlock Text="Кем выдан паспорт" />
			<TextBox Name="PassportIssuedByTextBox"
                     Text="{Binding PassportIssuedBy, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="Введите кем выдан"
                     TabIndex="11" />
			<TextBlock Text="{Binding PassportIssuedByError}" Foreground="Red" />

			<TextBlock Text="Адрес" />
			<TextBox Name="AddressTextBox"
                     Text="{Binding Address, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="Введите адрес (город, улица, дом)"
                     TabIndex="12" />
			<TextBlock Text="{Binding AddressError}" Foreground="Red" />

			<TextBlock Text="Телефон (+X (XXX) XXX-XX-XX)" />
			<TextBox Name="PhoneTextBox"
                     Text="{Binding Phone, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="+X (XXX) XXX-XX-XX"
                     TabIndex="13" />
			<TextBlock Text="{Binding PhoneError}" Foreground="Red" />

			<TextBlock Text="Наименование страховой медицинской организации" />
			<ComboBox Name="MedicalOrganizationComboBox"
                      SelectedItem="{Binding MedicalOrganization}"
                      ItemsSource="{Binding MedicalOrganizationOptions}"
                      SelectionChanged="ComboBox_SelectionChanged"
                      GotFocus="ComboBox_GotFocus"
                      KeyDown="InputField_KeyDown"
                      TabIndex="14" />
			<TextBlock Text="{Binding MedicalOrganizationError}" Foreground="Red" />

			<TextBlock Text="Полис ОМС (опционально, 16 цифр)" />
			<TextBox Name="MedicalPolicyTextBox"
                     Text="{Binding MedicalPolicy, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="16 цифр"
                     TabIndex="15" />
			<TextBlock Text="{Binding MedicalPolicyError}" Foreground="Red" />

			<TextBlock Text="Наблюдается ЛПУ" />
			<TextBox Name="MedicalFacilityTextBox"
                     Text="{Binding MedicalFacility, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="Введите название ЛПУ"
                     TabIndex="16" />
			<TextBlock Text="{Binding MedicalFacilityError}" Foreground="Red" />

			<TextBlock Text="Место работы" />
			<TextBox Name="WorkplaceTextBox"
                     Text="{Binding Workplace, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="Введите место работы"
                     TabIndex="17" />
			<TextBlock Text="{Binding WorkplaceError}" Foreground="Red" />

			<TextBlock Text="Адрес работы" />
			<TextBox Name="WorkAddressTextBox"
                     Text="{Binding WorkAddress, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="Введите адрес работы"
                     TabIndex="18" />
			<TextBlock Text="{Binding WorkAddressError}" Foreground="Red" />

			<TextBlock Text="Структурное подразделение" />
			<TextBox Name="DepartmentTextBox"
                     Text="{Binding Department, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="Введите подразделение"
                     TabIndex="19" />
			<TextBlock Text="{Binding DepartmentError}" Foreground="Red" />

			<TextBlock Text="Форма собственности" />
			<ComboBox Name="OwnershipFormComboBox"
                      SelectedItem="{Binding OwnershipForm}"
                      ItemsSource="{Binding OwnershipFormOptions}"
                      SelectionChanged="ComboBox_SelectionChanged"
                      GotFocus="ComboBox_GotFocus"
                      KeyDown="InputField_KeyDown"
                      TabIndex="20" />
			<TextBlock Text="{Binding OwnershipFormError}" Foreground="Red" />

			<TextBlock Text="ОКВЭД (XX.XX.XX)" />
			<TextBox Name="OkvedTextBox"
                     Text="{Binding Okved, UpdateSourceTrigger=PropertyChanged}"
                     TextChanged="HandleTextChanged"
                     TextInput="RestrictInput"
                     KeyDown="RestrictKeyDown"
                     Watermark="XX.XX.XX"
                     TabIndex="21" />
			<TextBlock Text="{Binding OkvedError}" Foreground="Red" />

			<TextBlock Text="Стаж работы" />
			<StackPanel Orientation="Horizontal">
				<TextBox Name="WorkExperienceYearsTextBox"
                         Text="{Binding WorkExperienceYears, UpdateSourceTrigger=PropertyChanged}"
                         TextChanged="HandleTextChanged"
                         TextInput="RestrictInput"
                         KeyDown="RestrictKeyDown"
                         Watermark="Годы"
                         Width="100"
                         TabIndex="22" />
				<TextBlock Text=" лет " />
				<TextBox Name="WorkExperienceMonthsTextBox"
                         Text="{Binding WorkExperienceMonths, UpdateSourceTrigger=PropertyChanged}"
                         TextChanged="HandleTextChanged"
                         TextInput="RestrictInput"
                         KeyDown="RestrictKeyDown"
                         Watermark="Месяцы"
                         Width="100"
                         TabIndex="23" />
				<TextBlock Text=" месяцев" />
			</StackPanel>
			<TextBlock Text="{Binding WorkExperienceYearsError}" Foreground="Red" />
			<TextBlock Text="{Binding WorkExperienceMonthsError}" Foreground="Red" />

			<!-- Кнопки -->
			<Button Name="PreviewButton"
                    Content="Предпросмотр"
                    Click="Preview_Click"
                    HorizontalAlignment="Center"
                    Margin="10"
                    TabIndex="24" />
			<Button Name="SaveButton"
                    Content="Сохранить"
                    Click="Save_Click"
                    HorizontalAlignment="Center"
                    Margin="10"
                    KeyDown="SaveButton_KeyDown"
                    TabIndex="25" />
			<Button Name="LoadExcelButton"
                    Content="Загрузить Excel"
                    Click="LoadExcel_Click"
                    HorizontalAlignment="Center"
                    Margin="10"
                    TabIndex="26" />
			<Button Name="BackToMenuButton"
                    Content="Назад в главное меню"
                    Click="BackToMenu_Click"
                    HorizontalAlignment="Center"
                    Margin="10"
                    TabIndex="27" />
		</StackPanel>
	</ScrollViewer>
</Window>